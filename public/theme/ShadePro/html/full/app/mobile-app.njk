{% import "sections/videos/video-4.njk" as Video %}
{% import "sections/content/content-7.njk" as ContentTwo %}
{% import "sections/testimonial/testimonial-7.njk" as Testimonial %}
{% import "sections/footers/_footer-3.njk" as Footer%}

{% block main %}
{{Header.render(header.six)}}

   <!-- Hero Area -->
   {{Hero.render(hero.six)}}
  <!-- <div class="" >
   -->

  <!-- Content section 1 -->
  <div class="content-section pt-6 pt-lg-19 pb-5 bg-default-4">
    <div class="container">
      <div class="row align-items-center">
        <div class="col-lg-6" data-aos="flip-right" data-aos-duration="1000" data-aos-once="true">
          <div class="content-img">
            <img src="{{root}}image/l6/png/l6-content1-img.png" alt="" class="w-100">
          </div>
        </div>
        <div class="col-10 col-lg-5 pl-xl-21">
          <div class="section-title content-text mb-13">
            <h2 class="title gr-text-3 mb-6">Collaborate with team members.</h2>
            <p class="gr-text-8">We share common trends and strategies for improving your rental income.</p>
          </div>
          <div class="content-widget">
            <div class="row">
              <div class="col-md-6 col-lg-11 col-xl-11" data-aos="fade-left" data-aos-duration="500" data-aos-once="true">
                <div class="single-widget mb-9">
                  <h3 class="w-title gr-text-7">Project Based Groups</h3>
