      <div class="wave-shape gr-abs-br">
        <img src="{{root}}image/svg/l6-footer-wave.svg" alt="" class="w-100 light-shape default-shape">
        <img src="{{root}}image/svg/l6-footer-wave-dark.svg" alt="" class="w-100 dark-shape">
      </div>
      <div class="pattern-shape gr-abs-br gr-z-index-n1" data-aos="fade-left" data-aos-duration="500" data-aos-delay="400" data-aos-once="true">
