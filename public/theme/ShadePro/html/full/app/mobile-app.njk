    <div class="gradient-sections-wrapper bg-gradient-1">
      <div class="shape">
        <img src="{{root}}image/svg/l6-video-wave.svg" alt="" class="w-100 light-shape default-shape">
        <img src="{{root}}image/svg/l6-video-wave-dark.svg" alt="" class="w-100 dark-shape">
      </div>
  
      <!-- Video area -->
      {{Video.render("./", "mt-n29 mt-lg-n35")}}

      
      <!-- Features section -->
      {{Feature.render("./", "dark-mode-texts")}}
 
  
      <!-- Testimonial section -->
      {{Testimonial.render()}}
      

