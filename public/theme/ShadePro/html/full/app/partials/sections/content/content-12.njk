{% macro render(root,options) %}
  <div class="content-section pt-11 pt-lg-20 pb-11 pb-lg-20 bg-default-6 {{options.wrapper_class}}" id={{options.id}}>
    <div class="container">
      <div class="row align-items-center justify-content-center">
        <div class="col-xs-11 col-sm-10 col-md-9 col-lg-6 col-xl-6 mb-6 mb-md-13 mb-lg-0">
          <div class="tel-content-image-group-1" data-aos="zoom-in" data-aos-duration="1200" data-aos-once="true">
            <div class="abs-image-1 responsive-scaling-2">
              <div class="animation-item">
                <img class="main-img" src="{{root}}image/telemedicine/png/content-1-img1.png"  alt="" id="an-item-2p1">
              </div>
            </div>
            <div class="abs-image-2 responsive-scaling-2">
             <div class="animation-item">
              <img class="main-img" src="{{root}}image/telemedicine/png/content-1-img2.png" alt="" id="an-item-2p2"/>
             </div>
            </div>
            <div class="gr-abs-center gr-z-index-n1">
              <img src="{{root}}image/telemedicine/png/content1-img-sub.png" alt="" class="responsive-scaling-2">
            </div>
          </div>
        </div>
        <div class="col-sm-10 col-md-9 col-lg-6 col-xl-5 offset-xl-1">
          <div class="content-text pr-lg-10">
            <h2 class="gr-text-4 mb-9">Make video call with your doctors, get treatment.</h2>
            <p class="gr-text-9 mb-0">We've made telemedicine simple and easy for you. Create your personal room and start practicing telemedicine today. Lorem Ipsum is simply dummy text of the printing and typesetting industry. </p>
            <a href="#" class="btn-link with-icon gr-text-blue gr-text-9 font-weight-bold mt-10">Check available doctors<i class="fas fa-arrow-right"></i></a>
          </div>
        </div>
      </div>
    </div>
  </div>


{% endmacro %}
