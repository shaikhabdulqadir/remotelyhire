{% macro render(options) %}
<div class="{{options.wrapperClasses}}" style="background-image:url({{options.backgroundImage}});">
  <div class="container">
    <div class="row align-items-center justify-content-center">
      <div class="col-lg-10 col-xl-9" data-aos="fade-up" data-aos-duration="1000" data-aos-once="true">
        <div class="hero-content section-title dark-mode-texts text-center">
          <h4 class="pre-title text-green gr-text-12 text-uppercase mb-10">Shared space in your town</h4>
          <h1 class="gr-text-2 font-weight-bold mb-12">Rent desk space in a
            shared office environment</h1>

          <!-- Search Component -->
          <div class="location-filter-form d-flex flex-wrap bg-white">
            <div class="single-input w-100 w-sm-50 w-lg-35 px-5 py-3 py-md-7">
              <select name="location" id="location" class="nice-select h-px-50 text-blackish-blue h-md-100 arrow-2 location-select">
                <option value="" data-display="Select Location">Select Location</option>
                <option value="">United States of America</option>
                <option value="">United Arab Emirates</option>
                <option value="">Bangladesh</option>
                <option value="">Pakistan</option>
              </select>
            </div>
            <div class="single-input border-right-0 w-100 w-sm-50 w-lg-35 px-5  d-flex align-items-center py-3 py-md-7">
              <div class="h-px-50 position-relative date-picker w-100 arrow-box-dropdown">
                
                <input class="form-control date-picker-input
                border-0  rounded-0 h-100" id="datepicker" placeholder="Select Date">
              </div>
              
            </div>

            <div class="single-input  w-100 w-lg-30 d-flex align-items-center justify-content-center border-0 py-4 py-md-7">
              <button type="submit" class="btn btn-primary search-btn">Search Place</button>
            </div>
          </div>
          
          <a data-fancybox href="https://www.youtube.com/watch?v=_sI_Ps7JSEk" class="video-link gr-text-color  mt-11 d-block">
            <span class="d-inline-flex mr-2 gr-text-color">
              <i class="far fa-play-circle gr-text-9"></i>
            </span>
            <span class="gr-text-9 mb-0">Take virtual tour of our spaces</span>
          </a>
        </div>
      </div>
    </div>
  </div>
  <a href="#second-section" class="circle-40 border border-white gr-abs-bx-center mx-auto mt-13 goto">
    <i class="fas fa-chevron-down text-white mx-auto gr-text-8"></i>
  </a>
</div>
{% endmacro %}


