{% macro render(options) %}
    
<div class="footer-section pt-15 pt-lg-25 pb-lg-21 dark-mode-texts {{ options.wrapper_classes }}">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-3 col-md-4">
