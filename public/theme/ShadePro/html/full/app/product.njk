{% extends "template.njk" %}
{% import "sections/header/_header.njk" as Header %}
{% import "sections/hero-area/hero-7.njk" as Hero %}
{% import "sections/content/content-8.njk" as Content %}
{% import "sections/content/content-9.njk" as ContentThree %}
{% import "sections/footers/_footer-4.njk" as Footer %}
{% block main %}
{{Header.render(header.seven)}}
<!-- Hero Area -->
{{Hero.render(hero.seven)}}

<!-- Content section 1 -->
{{Content.render("./", "pt-15 pb-11 pb-lg-25 pt-lg-27 bg-default-2")}}


<!-- Content section 2 -->
<div class="content-section pt-15 pb-11 pt-lg-25 pb-lg-24 pt-xl-30 pb-xl-28 position-relative bg-blue dark-mode-texts overflow-hidden">
  <div class="container">
    <div class="row align-items-center justify-content-md-center">
      <div class="col-sm-10 col-lg-5 order-lg-2 position-static">
        <div class="content-img position-relative gr-abs-lg gr-abs-lg-ry-center w-lg-50 text-center text-lg-right">
          <img class="h-100 w-100 w-lg-75" src="{{root}}image/l7/jpg/content2-img-big.jpg" alt="">
        </div>
      </div>
      <div class="col-sm-11 col-lg-7 order-lg-1">
        <div class="row">
          <div class="col-lg-10 col-xl-9">
            <div class="content-text mb-9 mb-lg-18 mt-11 mt-lg-0">
              <h2 class="title gr-text-3 mb-6">Comfortable buds with better sound.</h2>
              <p class="gr-text-8 mb-9 pr-11">We share common trends and strategies for improving your rental income.</p>
            </div>
            <div class="testimonial-wrapper mt-6 mt-md-0">
              <div class="single-testimonial media py-6" data-aos="fade-left" data-aos-duration="800" data-aos-once="true">
                <div class="media-icon mr-8">
                  <img class=" circle-sm-2" src="{{root}}image/l7/jpg/testimonial-1.jpg" alt="">
                </div>
                <div class="media-body">
                  <p class="gr-text-7 mb-0 pr-xl-6 gr-text-color mb-8">“You made it so simple. My new site is so much faster and easier to work with than my old site.”</p>
                  <span class="name gr-text-11 gr-text-color-opacity">Rhoda Brady</span>
                </div>
              </div>              
            </div>
          </div>
        </div>
        
        
      </div>
    </div>
  </div>
</div>


<!-- Content section 3 -->
{{ContentThree.render("./", "pt-13 pb-15 py-lg-23 py-xl-29 bg-pattern pattern-3 bg-blackish-blue dark-mode-texts")}}



<!-- Pricing section -->
<div class="pricing-section bg-default-4 pt-13 pb-5 pt-lg-26 pb-lg-29">
  <div class="container">
    <div class="row align-items-center justify-content-center">
      <div class="col-xl-6 col-lg-7 col-md-8">
        <div class="section-title text-center mb-15 mb-lg-19">
          <h2 class="title gr-text-3 mb-7">Get your airpod now.</h2>
          <p class="gr-text-8 mb-0 px-11">We share common trends and strategies for improving your rental income.</p>
        </div>
      </div>
    </div>
    <div class="row justify-content-center">
      <div class="col-sm-9 col-md-6 col-lg-4 mb-12 mb-lg-0">
        <div class="single-product d-flex flex-column px-7 text-center">
          <div class="product-img mb-9">
            <img class="w-100 rounded-8" src="{{root}}image/l7/jpg/product-thumb-img1.jpg" alt="">
          </div>
          <h4 class="gr-text-7 mb-6">Midnight Green</h4>
          <p class="gr-text-9 mb-9">AirPods (2nd gen) iPhone Colors with Wireless Charging Case</p>
          <a href="" class="btn btn-primary gr-hover-y gr-text-11 mw-75 mx-auto">Buy now - $99</a>
        </div>
      </div>
      <div class="col-sm-9 col-md-6 col-lg-4 mb-12 mb-lg-0">
        <div class="single-product d-flex flex-column px-7 text-center">
          <div class="product-img mb-9">
            <img class="w-100 rounded-8" src="{{root}}image/l7/jpg/product-thumb-img2.jpg" alt="">
          </div>
          <h4 class="gr-text-7 mb-6">Silver</h4>
          <p class="gr-text-9 mb-9">AirPods (2nd gen) iPhone Colors with Wireless Charging Case</p>
          <a href="" class="btn btn-primary gr-hover-y gr-text-11 mw-75 mx-auto">Buy now - $99</a>
        </div>
      </div>
      <div class="col-sm-9 col-md-6 col-lg-4 mb-12 mb-lg-0">
        <div class="single-product d-flex flex-column px-7 text-center">
          <div class="product-img mb-9">
            <img class="w-100 rounded-8" src="{{root}}image/l7/jpg/product-thumb-img3.jpg" alt="">
          </div>
          <h4 class="gr-text-7 mb-6">Gold</h4>
          <p class="gr-text-9 mb-9">AirPods (2nd gen) iPhone Colors with Wireless Charging Case</p>
          <a href="" class="btn btn-primary gr-hover-y gr-text-11 mw-75 mx-auto">Buy now - $99</a>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- CTA section -->
<div class="cta-section parallax-section-750 bg-parallax-image" style="background-image:url({{root}}image/l7/jpg/cta-bg.jpg)">
  <div class="container-fluid pr-0 pl-0">
    {# <img class="position-fixed" src="" class="w-100 px-n4" alt=""> #}
  </div>
</div>

<!-- Footer section -->
{{Footer.render(footer.seven)}}


{% endblock %}
